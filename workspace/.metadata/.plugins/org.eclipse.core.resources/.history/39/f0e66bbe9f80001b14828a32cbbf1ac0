package ex02_2d_array;

public class Quiz01 {

// 인구조사
//apt에 사는 사람 수 를 초기화 해 놓고,
//전체 사람의 수를 출력하기
	
//출력예시
//1층에 3명이 삽니다.
//2층에 7명이 삽니다.
//3층에 6명이 삽니다.
//4층에 5명이 삽니다.
//5층에 7명이 삽니다.
//apt에 28명이 삽니다.
	
	public static void main(String[] args) {
		
		//5층, 2호
		int[][] apt = {
				{1,2},
				{5,2},
				{3,3},
				{1,4},
				{5,2}
		};
		int[] floor;
		for(int i= 0 ; i < apt.length ; i++) {
			int[i] floor= new int[apt.length];
		}
	}

}
