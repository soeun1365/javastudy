package ex02_String;

public class Ex02_StringBuilder {

	public static void main(String[] args) {
		
		// java.lang.StringBuilder		//import가 필요없음
		
		// 문자열 생성
		StringBuilder sb = new StringBuilder("hello");
		
		// 문자열 추가 (핵심 메소드)
		sb.append(" ").append("java");

		System.out.println(sb);
	}

}
