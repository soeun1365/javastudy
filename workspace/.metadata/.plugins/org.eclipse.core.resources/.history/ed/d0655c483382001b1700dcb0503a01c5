package ex03_overloading;
/*
 	오버로딩(over roading)
 	1. 이름이 같고, 매개변수가 다른 메소드가 있음을 의미
 	2. 결과타입은 오버로딩과 상관이 없습니다. 같아도 되고, 달라도됨
 	
 */
public class Rectangle {
	
	//field
	int width;
	int height;
	
	//method
	void set(int w, int h) {
			if(w > 0) {
				int width = w;
			}
			int height = h;
		}
	
	//method
	void set(int n) {
		int width = n;
		int height = n;
	}
	
	//method
	int getArea() {
		return width * height;
		
		
	}
}
