package ex02_loop;
// 2*1=2  3*1=3 ... 9&1=9
// 2*2=4  3*2=6 ... 9&2=18
// 2*3=6  3*3=9 ... 9&3=27
// ...
// 2*9=18  3*9=27 ... 9&9=81

public class Quiz04 {

	public static void main(String[] args) {
		for(int i=2 ; i<=9 ; i++) {
			for(int j=1 ; j<=9 ; j++) {
				System.out.println(j + "*" + i + "=" + (j*i));
			}
		}

	}

}
