package ex01_random;

//	5 x 5 크기의 빙고 생성하기
//	1) 1~25를 bingo 배열에 순서대로 저장해 두고,
//	2) 섞는다.
public class Quiz05 {
	
	public static void main(String[] args) {
		//final 상수 : 변하지 않는 값을 저장해 두는 경우에 사용
		final int SIZE = 5;
		// SIZE =6;		-> 오류남, 변할 수 없는 값이기 때문
		
		int[][] bingo = new int[SIZE][SIZE];
		
		for(int i=0 ; i< SIZE; i++) {
			for(j=0 ; j<SIZE ; j++) {
				bingo[i][j] = (i * SIZE)
			}
		}
	}
}
