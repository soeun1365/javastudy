package ex02_loop;

public class Ex07_label {

	public static void main(String[] args) {
		
		//dan * n = (dan * n)
		for(int dan = 2 ; dan <= 9 ; dan++) {
			for(int n = 1 ; n <= 9 ; n++) {
				if(dan==5 && n==5) {
					break;
				}
				System.out.println(dan + "*" + n + "=" + (dan*n));
			}
		}

	}

}
