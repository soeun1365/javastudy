package ex02_loop;

public class Ex04_nested_loop {

	public static void main(String[] args) {
		
		for(int out = 1 ; out<=10; out++) {
			for(int in = 10 ; in <= 50 ; in+=10) {
				System.out.println(out + "," + in);
			}
		}
		
		
		// 1주차 1일차 1교시입니다.
		// 1주차 1일차 2교시입니다.
		//...
		// 총 3주차, 5일, 8교시 진행

		for(int week = 0 ; week<3 ; week++) {
			for()
		}
	}

}
