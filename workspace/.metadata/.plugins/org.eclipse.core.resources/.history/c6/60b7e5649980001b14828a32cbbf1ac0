package ex02_2d_array;

public class Ex02_2d_reference {

	public static void main(String[] args) {
		int[][] a = {
				{1,2},
				{3,4,5,6},
				{7,8}
		};

		System.out.println(a);
		System.out.println(a[0]);	//a[0]는 {1,2} 입니다. 1차원배열
		System.out.println(a[1]);
		System.out.println(a[2]);
		
	}

}
